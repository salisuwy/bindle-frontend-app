{
  "name": "bindle-vue",
  "private": true,
  "version": "0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-staging": "vite build --mode staging",
    "build-prod": "vite build --mode production",
    "type-check": "vue-tsc --build --force --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "deploy-prod": "eval $(ssh-agent -s) && ssh-add /root/.ssh/bindle_repo_keys_FE && export PATH=$PATH:/root/.nvm/versions/node/v18.19.1/bin && cd /home/bindle-frontend-app && git fetch origin && git checkout main && git pull origin main && npm ci && npm run build-prod"
  },
